<!DOCTYPE html>
<html>
<head>
    {%- block meta -%}
        <meta charset="utf-8">
        <meta http-equiv="x-ua-compatible" content="ie=edge">
        <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    {%- endblock -%}

    {%- block title -%}
        <title>Citrus - Admin</title>
    {%- endblock -%}

    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('/admin/vendor/AdminLTE-3/plugins/bootstrap/css/bootstrap.min.css') }}">
        <link rel="stylesheet" href="{{ asset('/admin/vendor/AdminLTE-3/plugins/colorpicker/bootstrap-colorpicker.min.css') }}">
        <link rel="stylesheet" href="{{ asset('/admin/vendor/AdminLTE-3/plugins/iCheck/all.css') }}">
        <link rel="stylesheet" href="{{ asset('/admin/vendor/font-awesome/css/font-awesome.min.css') }}">
        <link rel="stylesheet" href="{{ asset('/admin/vendor/Ionicons/css/ionicons.min.css') }}">
        <link rel="stylesheet" href="{{ asset('/admin/vendor/AdminLTE-3/plugins/select2/select2.min.css') }}">
        <link rel="stylesheet" href="{{ asset('/admin/vendor/AdminLTE-3/dist/css/adminlte.min.css') }}">
        <link rel="stylesheet" href="{{ asset('/admin/css/skin-custom.css') }}">
        <link rel="stylesheet" href="{{ asset('/admin/vendor/combobox/css/jquery.ajax-combobox.min.css') }}">
        <link rel="stylesheet" href="{{ asset('/admin/vendor/jstree/dist/themes/default/style.min.css') }}">

        <link rel="stylesheet" href="{{ asset('/admin/vendor/datePicker/css/bootstrap-datepicker.css') }}" />
    {% endblock %}

    {%- block favicon -%}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>
    {%- endblock -%}

    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700">

    <script src="{{ asset('admin/vendor/AdminLTE-3/plugins/jquery/jquery.min.js') }}"></script>
</head>

{% block body %}
    <body class="hold-transition sidebar-mini">
    <div class="wrapper">
        <nav class="main-header navbar navbar-expand bg-white navbar-light border-bottom">
            <!-- Left navbar links -->
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" data-widget="pushmenu" href="#"><i class="fa fa-bars"></i></a>
                </li>
                <li class="nav-item d-none d-sm-inline-block">
                    <a href="#" class="nav-link">Home</a>
                </li>
            </ul>
        </nav>
        <!-- Main Sidebar Container -->
        <aside class="main-sidebar sidebar-dark-primary elevation-4">
            <!-- Brand Logo -->

            <a href="#" class="brand-link ">
                <img src="https://i.citrus.ua/pictures/all/svg/citrus_brand_new_logo.svg" alt="Logo" class="brand-image">
                <span class="brand-text font-weight-light">&nbsp;</span>
            </a>

            <!-- Sidebar -->
            <div class="sidebar">
                <!-- Sidebar Menu -->
                <nav class="mt-2">
                    {{ include('admin/sidebar-menu.html.twig') }}
                </nav>
                <!-- /.sidebar-menu -->
            </div>
            <!-- /.sidebar -->
        </aside>

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            {% block contentHeader %}
                <section class="content-header">
                    <div class="container-fluid">
                        <div class="row mb-2">
                            <div class="col-sm-6">
                                <h1>
                                    {{ data is defined ? data.title : '' }}
                                    <small>{{ data is defined ? data.description : '' }}</small>
                                </h1>
                            </div>
                        </div>
                    </div>
                </section>
            {% endblock %}

            <!-- Main content -->
            <section class="content">
                <div class="container-fluid">
                    {#{% block flashes %}#}
                    {#{% for message in app.flashes('success') %}#}
                    {#<div class="flash-msg-wrapper success">#}
                    {#<div class="flash-msg" style="display: none">#}
                    {#{{ message | trans }}#}
                    {#</div>#}
                    {#</div>#}
                    {#{% endfor %}#}

                    {#{% for message in app.flashes('info') %}#}
                    {#<div class="flash-msg-wrapper info">#}
                    {#<div class="flash-msg" style="display: none">#}
                    {#{{ message | trans }}#}
                    {#</div>#}
                    {#</div>#}
                    {#{% endfor %}#}
                    {#{{ include('/admin/components/flash.html.twig') }}#}
                    {#{% endblock %}#}
                    {% block content %}{% endblock %}
                </div>
            </section>
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->

        <!-- Main Footer -->
        <footer class="main-footer">
            <!-- Default to the left -->
            <strong>Copyright &copy; {{ date().format('Y') }} <a href="#">CITRUS</a>.</strong>
        </footer>
    </div>
    {% block includeApplication %}
        <script src="{{ asset('admin/vendor/AdminLTE-3/plugins/jQueryUI/jquery-ui.min.js') }}"></script>
        <script src="{{ asset('admin/vendor/AdminLTE-3/plugins/bootstrap/js/bootstrap.bundle.min.js') }}"></script>
        <script src="{{ asset('admin/vendor/AdminLTE-3/plugins/fastclick/fastclick.min.js') }}"></script>
        <script src="{{ asset('admin/vendor/AdminLTE-3/dist/js/adminlte.min.js') }}"></script>
        `
        <script src="{{ asset('admin/vendor/jQuery-validation/jquery-validation.min.js') }}"></script>

        <script src="{{ asset('admin/vendor/AdminLTE-3/plugins/select2/select2.full.min.js') }}"></script>
        {#<script src="{{ asset('bundles/tetranzselect2entity/js/select2entity.js') }}"></script>#}
        <script src="{{ asset('admin/vendor/tmp/tetranzselect2entity/js/select2entity.js') }}"></script>

        <script src="{{ asset('admin/vendor/AdminLTE-3/plugins/iCheck/icheck.min.js') }}"></script>
        {#<script src="{{ asset('admin/vendor/AdminLTE-3/plugins/ckeditor/ckeditor.js') }}"></script>#}
        <script src="{{ asset('admin/vendor/ckeditor/ckeditor.js') }}"></script>
        <script src="{{ asset('admin/vendor/AdminLTE-3/dist/js/demo.js') }}"></script>

        {#<script src="{{ asset('/js/libs/chartjs/Chart.bundle.js') }}"></script>#}
        {#<script src="{{ asset('/js/libs/datePicker/js/bootstrap-datepicker.js') }}"></script>#}
        {#<script src="{{ asset('/js/libs/chartjs/Chartjs-plugin-datalabels.min.js') }}"></script>#}

        <script src="{{ asset('/admin/vendor/combobox/js/jquery.ajax-combobox.min.js') }}"></script>
        <script src="{{ asset('/admin/vendor/jstree/dist/jstree.min.js') }}"></script>

        <script src="{{ asset('/admin/vendor/datePicker/js/bootstrap-datepicker.js') }}"></script>

        {#<script src="{{ asset('/js/tinymce/tinymce.min.js') }}"></script>#}

        <script src="{{ asset('admin/js/my.js') }}"></script>
    {% endblock %}

    {% block javascripts %} {% endblock %}
    </body>
{% endblock %}
</html>
